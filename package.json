{
    "name": "project-skeleton",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "lint": "ng lint",
        "prepare": "husky",
        "pre-commit": "npx lint-staged",
        "storybook": "ng run project-skeleton:storybook",
        "build-storybook": "ng run project-skeleton:build-storybook",
        "generate-icons": "svg-to-ts-constants"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^15.0.0",
        "@angular/common": "^15.0.0",
        "@angular/compiler": "^15.0.0",
        "@angular/core": "^15.0.0",
        "@angular/forms": "^15.0.0",
        "@angular/platform-browser": "^15.0.0",
        "@angular/platform-browser-dynamic": "^15.0.0",
        "@angular/router": "^15.0.0",
        "rxjs": "~7.5.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.12.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^15.0.4",
        "@angular-eslint/builder": "16.3.1",
        "@angular-eslint/eslint-plugin": "16.3.1",
        "@angular-eslint/eslint-plugin-template": "16.3.1",
        "@angular-eslint/schematics": "16.3.1",
        "@angular-eslint/template-parser": "16.3.1",
        "@angular/cli": "^16.2.13",
        "@angular/compiler-cli": "^15.0.0",
        "@chromatic-com/storybook": "^1.3.2",
        "@storybook/addon-essentials": "^8.0.8",
        "@storybook/addon-interactions": "^8.0.8",
        "@storybook/addon-links": "^8.0.8",
        "@storybook/angular": "^8.0.8",
        "@storybook/blocks": "^8.0.8",
        "@storybook/test": "^8.0.8",
        "@types/jasmine": "~4.3.0",
        "@typescript-eslint/eslint-plugin": "5.62.0",
        "@typescript-eslint/parser": "5.62.0",
        "autoprefixer": "^10.4.19",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-simple-import-sort": "^12.1.0",
        "eslint-plugin-storybook": "^0.8.0",
        "globals": "^15.0.0",
        "husky": "^9.0.11",
        "jasmine-core": "~4.5.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.0.0",
        "lint-staged": "^15.2.2",
        "postcss": "^8.4.39",
        "prettier": "3.2.5",
        "storybook": "^8.0.8",
        "svg-to-ts": "^12.0.0",
        "tailwindcss": "^3.4.4",
        "typescript": "~4.8.2"
    },
    "svg-to-ts": {
        "srcFiles": [
            "./src/assets/icons/**/*.svg"
        ],
        "outputDirectory": "./src/assets/icons",
        "interfaceName": "SvgIcon",
        "typeName": "svgIcon",
        "prefix": "svg",
        "fileName": "icons.constants",
        "additionalModelFile": "./src/assets/icons",
        "compileSources": true
    },
    "lint-staged": {
        "**/*": "prettier --write --ignore-unknown"
    }
}
